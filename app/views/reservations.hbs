<!-- Navigation -->
{{> nav }}
<!-- Main Container -->
<main class="main-container">

    <!-- Section: About -->
    <section id="about" class="section-2">
        <div class="container">

            <!-- Section Header Row -->
            <div class="row">
                <div class="col-12">
                {{#unless bookings}}
                  <header class="section-header">
                    <div class="section-heading">
                      <div class="section-title">There are no reservation bookings!</div>
                    </div>
                  </header>
                {{else}}
                <header class="section-header">
                  <div class="section-heading">
                    <div class="section-title">Reservations</div>
                  </div>
                </header>
                <table class="table table-striped">
                    <thead>
                    <tr>
                        <th scope="col">#ID</th>
                        <th scope="col">Name</th>
                        <th scope="col">Email</th>
                        <th scope="col">Date</th>
                        <th scope="col">Time</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each bookings}}
                        <tr>
                          <th scope="row">{{id}}</th>
                          <td>{{name}}</td>
                          <td>{{email}}</td>
                          <td>{{date}}</td>
                          <td>{{time}}</td>
                      </tr>
                    {{/each}}

                    </tbody>
                </table>
                {{/unless}}
                </div>
            </div>
            <!-- /Section Header Row -->
    </div>
    </section>
</main>
<!-- /Main Container -->
{{> footer }}

<!-- Overlay -->
<div class="overlay">
    <div class="loader"></div>
</div>
<!-- /Overlay -->


<!-- Scripts -->
<script src="/javascripts/jquery.stellar.min.js"></script>
<script src="/javascripts/popper.min.js"></script>
<script src="/javascripts/jquery-3.3.1.min.js"></script>
<script src="/components/bootstrap/js/bootstrap.min.js"></script>
<script src="/components/owl-carousel/owl.carousel.min.js"></script>
<script src="/components/magnific-popup/jquery.magnific-popup.min.js"></script>
<script src="/javascripts/jquery.appear.js"></script>
<script src="/javascripts/jquery.animateNumber.min.js"></script>
<script src="/javascripts/circle-progress.min.js"></script>
<script src="/components/prism/prism.js"></script>
<script src="/javascripts/general.js"></script>
<!-- /Scripts -->


